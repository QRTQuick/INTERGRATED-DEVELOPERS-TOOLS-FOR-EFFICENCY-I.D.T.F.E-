<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.idtfe.controllers.ProjectExplorerController" spacing="6.0" style="-fx-padding:10;">
    <Label text="Project Explorer" style="-fx-font-weight:bold; -fx-font-size:14;" />
    <TreeView fx:id="projectTree" VBox.vgrow="ALWAYS" />
    <ToolBar>
        <Button fx:id="refreshBtn" text="Refresh" />
        <Button fx:id="openBtn" text="Open" />
    </ToolBar>
</VBox>
